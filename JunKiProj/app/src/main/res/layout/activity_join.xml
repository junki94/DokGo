<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/je1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/je2"
        android:layout_marginTop="36dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="118dp" />

    <EditText
        android:id="@+id/je2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/je1"
        android:layout_marginEnd="59dp"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="286dp" />

    <EditText
        android:id="@+id/je3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/je2"
        android:layout_below="@+id/je2"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="233dp" />

    <EditText
        android:id="@+id/je4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/je3"
        android:layout_below="@+id/je3"
        android:layout_marginTop="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="194dp" />

    <EditText
        android:id="@+id/je5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/je4"
        android:layout_below="@+id/je4"
        android:layout_marginTop="38dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="162dp" />

    <TextView
        android:id="@+id/jtxt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/je2"
        android:layout_marginEnd="23dp"
        android:layout_toStartOf="@+id/je1"
        android:text="Name" />

    <TextView
        android:id="@+id/jtxt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/je2"
        android:layout_alignStart="@+id/jtxt1"
        android:text="ID" />

    <TextView
        android:id="@+id/jtxt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/je3"
        android:layout_alignStart="@+id/jtxt2"
        android:text="Pass" />

    <TextView
        android:id="@+id/jtxt4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/je4"
        android:layout_alignStart="@+id/jtxt3"
        android:layout_marginBottom="11dp"
        android:text="Pass2" />

    <TextView
        android:id="@+id/jtxt5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/jtxt4"
        android:layout_alignTop="@+id/je5"
        android:layout_marginTop="12dp"
        android:text="Phone" />

    <Button
        android:id="@+id/jbutton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/je5"
        android:layout_below="@+id/je5"
        android:layout_marginStart="19dp"
        android:layout_marginTop="40dp"
        android:onClick="onButtonJOIN"
        android:text="회원 가입" />
</RelativeLayout>